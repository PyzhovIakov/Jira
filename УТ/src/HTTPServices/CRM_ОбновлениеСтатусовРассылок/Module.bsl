Функция WebHookPOST(Запрос)
	ИмяМетода = Запрос.ПараметрыURL.Получить("ИмяМетода");
	Если НРег(ИмяМетода) = "unione" Тогда
		Менеджер = CRM_ИнтеграцияССервисамиРассылок.МодульМенеджера(ИмяМетода);
		Результат = Менеджер.ОбработатьWebHook(Запрос);
	КонецЕсли;
	
	Ответ = Новый HTTPСервисОтвет(200);
	Ответ.УстановитьТелоИзСтроки("OK");
	Возврат Ответ;
КонецФункции

