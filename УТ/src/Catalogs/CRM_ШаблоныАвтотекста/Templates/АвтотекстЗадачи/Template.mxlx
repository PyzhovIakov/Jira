		<row>
			<c>
				<c>
					<f>9</f>
					<parameter>Документ оплаты</parameter>
				</c>
			</c>
			<c>
				<c>
					<f>9</f>
					<parameter>ДОКУМЕНТ_ОПЛАТЫ</parameter>
				</c>
			</c>
			<c>
				<c>
					<f>10</f>
					<parameter>Если CRM_ОбщегоНазначенияПовтИсп.ЭтоCRM() Тогда
	Запрос = Новый Запрос;
	Если ТипЗнч(Объект) = Тип("Структура") Тогда
		Запрос.УстановитьПараметр("БизнесПроцесс",Объект.Задача.БизнесПроцесс);
		Запрос.УстановитьПараметр("Задача",Объект.Задача.Ссылка);
	Иначе
		Запрос.УстановитьПараметр("БизнесПроцесс",Объект.БизнесПроцесс);
		Запрос.УстановитьПараметр("Задача",Объект.Ссылка);
	КонецЕсли;
	Запрос.Текст = "ВЫБРАТЬ ПЕРВЫЕ 1
	               |	CRM_ОбъектыПоБизнесПроцессам.Объект КАК Счет,
	               |	CRM_ОбъектыПоБизнесПроцессам.ДатаРегистрации КАК ДатаРегистрации
	               |ИЗ
	               |	РегистрСведений.CRM_ОбъектыПоБизнесПроцессам КАК CRM_ОбъектыПоБизнесПроцессам
	               |ГДЕ
	               |	CRM_ОбъектыПоБизнесПроцессам.БизнесПроцесс = &amp;БизнесПроцесс
	               |	И CRM_ОбъектыПоБизнесПроцессам.Задача = &amp;Задача
	               |	И CRM_ОбъектыПоБизнесПроцессам.Объект ССЫЛКА Документ.CRM_СчетНаОплатуПокупателю
	               |
	               |УПОРЯДОЧИТЬ ПО
	               |	ДатаРегистрации УБЫВ";
	Выборка = Запрос.Выполнить().Выбрать();
	Если Выборка.Следующий() Тогда
		Результат = НСтр("ru = 'Счет на оплату покупателю от '") +Формат(Выборка.Счет.Дата,"ДФ='дд.ММ.гг'") + НСтр("ru = ' на сумму '") + CRM_ОбщегоНазначенияКлиентСервер.ФорматСумм(Выборка.Счет.СуммаДокумента,Выборка.Счет.ВалютаДокумента);
	Иначе
		Результат = "";
	КонецЕсли;
Иначе
	Результат = "";
КонецЕсли;</parameter>
				</c>
			</c>
