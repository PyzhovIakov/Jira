					<f>15</f>
					<parameter>ТемпПол = "";
Если ЗначениеЗаполнено(Объект.КонтактноеЛицо) Тогда
	Результат = Объект.КонтактноеЛицо;
	ТемпПол = Объект.КонтактноеЛицо.Пол;
Иначе
	Результат = "";
	Если ЗначениеЗаполнено(Объект.Партнер.CRM_ОсновноеКонтактноеЛицо) Тогда
		Результат = Объект.Партнер.CRM_ОсновноеКонтактноеЛицо;
		ТемпПол = Объект.Партнер.CRM_ОсновноеКонтактноеЛицо.Пол;
	Иначе
		Запрос = Новый Запрос;
		Запрос.Текст = "ВЫБРАТЬ ПЕРВЫЕ 1
               |	КонтактныеЛица.Ссылка КАК КонтактноеЛицо
               |ИЗ
               |	Справочник.КонтактныеЛицаПартнеров КАК КонтактныеЛица
               |ГДЕ
               |	(НЕ КонтактныеЛица.ПометкаУдаления)
               |	И КонтактныеЛица.Владелец = &amp;Владелец";
		Запрос.УстановитьПараметр("Владелец", Объект.Партнер);

		Выборка = Запрос.Выполнить().Выбрать();
		Если Выборка.Следующий() Тогда
			Результат = Выборка.КонтактноеЛицо;
			ТемпПол = Выборка.КонтактноеЛицо.Пол;
		КонецЕсли;
	КонецЕсли;
КонецЕсли;
Результат = Строка(Результат);
Если ЗначениеЗаполнено(ТемпПол) Тогда
	Результат = ПолучитьСклоненияСтроки(Результат, "ПЛ="+Строка(ТемпПол), "ПД=Родительный");
	Результат = Результат[0];
КонецЕсли;</parameter>
