	<rowsItem>
		<index>7</index>
		<row>
			<c>
				<c>
					<f>9</f>
					<parameter>Контактные телефоны</parameter>
				</c>
			</c>
			<c>
				<c>
					<f>9</f>
					<parameter>КОНТАКТНЫЕ_ТЕЛЕФОНЫ</parameter>
				</c>
			</c>
			<c>
				<c>
					<f>9</f>
					<parameter>Результат = "";
Если ЗначениеЗаполнено(Объект.КонтактноеЛицо) Тогда
	КонтактТега = Объект.КонтактноеЛицо;
ИначеЕсли ЗначениеЗаполнено(Объект.Партнер) И Объект.Партнер.ЮрФизЛицо = Перечисления.КомпанияЧастноеЛицо.ЧастноеЛицо Тогда
	КонтактТега = Объект.Партнер;
ИначеЕсли НЕ ЗначениеЗаполнено(Объект.Партнер) Тогда
	КонтактТега = Объект.ПотенциальныйКлиент;	
Иначе
	КонтактТега = Неопределено;
КонецЕсли;
Если КонтактТега &lt;&gt; Неопределено Тогда
	КИКонтактаТега = УправлениеКонтактнойИнформацией.КонтактнаяИнформацияОбъекта(КонтактТега,,,Ложь);
	Если ТипЗнч(КИКонтактаТега) = Тип("ТаблицаЗначений") Тогда
		Для каждого СтрокаКИКонтактаТега из КИКонтактаТега Цикл
			Если СтрокаКИКонтактаТега.Тип&lt;&gt;Перечисления.ТипыКонтактнойИнформации.Телефон Тогда Продолжить КонецЕсли;
			Результат = Результат + ?(Результат = "", "", Символы.ПС)
				+ СтрокаКИКонтактаТега.Вид + ": " + СтрокаКИКонтактаТега.Представление;
		КонецЦикла;
	КонецЕсли;
КонецЕсли;</parameter>
				</c>
			</c>
			<c>
				<c>
					<f>11</f>
					<parameter>АвтотекстИнтереса</parameter>
				</c>
			</c>
			<c>
				<c>
					<f>10</f>
					<parameter>Результат = "";
Если ЗначениеЗаполнено(Объект.КонтактноеЛицо) Тогда
	КонтактТега = Объект.КонтактноеЛицо;
ИначеЕсли ЗначениеЗаполнено(Объект.Партнер) И Объект.Партнер.ЮрФизЛицо = Перечисления.КомпанияЧастноеЛицо.ЧастноеЛицо Тогда
	КонтактТега = Объект.Партнер;
ИначеЕсли НЕ ЗначениеЗаполнено(Объект.Партнер) Тогда
	КонтактТега = Объект.ПотенциальныйКлиент;	
Иначе
	КонтактТега = Неопределено;
КонецЕсли;
Если КонтактТега &lt;&gt; Неопределено Тогда
	КИКонтактаТега = УправлениеКонтактнойИнформацией.КонтактнаяИнформацияОбъекта(КонтактТега,,,Ложь);
	Если ТипЗнч(КИКонтактаТега) = Тип("ТаблицаЗначений") Тогда
		Для каждого СтрокаКИКонтактаТега из КИКонтактаТега Цикл
			Если СтрокаКИКонтактаТега.Тип&lt;&gt;Перечисления.ТипыКонтактнойИнформации.Телефон Тогда Продолжить КонецЕсли;
			Результат = Результат + ?(Результат = "", "", Символы.ПС)
				+ СтрокаКИКонтактаТега.Вид + ": " + СтрокаКИКонтактаТега.Представление;
		КонецЦикла;
	КонецЕсли;
КонецЕсли;</parameter>
				</c>
			</c>
		</row>
	</rowsItem>
