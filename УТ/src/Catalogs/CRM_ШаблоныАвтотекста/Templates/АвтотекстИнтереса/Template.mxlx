			<c>
				<c>
					<f>14</f>
					<parameter>Результат = "";

Если ЗначениеЗаполнено(Объект.Ответственный) И ТипЗнч(Объект.Ответственный) = Тип("СправочникСсылка.Пользователи") Тогда
	Ответственный = Объект.Ответственный;
Иначе
	Ответственный = Пользователи.АвторизованныйПользователь();
КонецЕсли;
	
Попытка
	ВидКИ = Справочники.ВидыКонтактнойИнформации["EmailПользователя"];
	НайденаяСтрока = Ответственный.КонтактнаяИнформация.Найти(ВидКИ,"Вид");
Исключение
	НайденаяСтрока = Ответственный.КонтактнаяИнформация.Найти(Перечисления.ТипыКонтактнойИнформации["АдресЭлектроннойПочты"],"Тип");
КонецПопытки;
Если НЕ НайденаяСтрока = Неопределено Тогда
	Результат = НайденаяСтрока.Представление;
КонецЕсли;</parameter>
				</c>
			</c>
			<c>
				<c>
					<f>11</f>
					<parameter>АвтотекстИнтереса</parameter>
				</c>
			</c>
			<c>
				<c>
					<f>14</f>
					<parameter>Результат = "";
	
Если ЗначениеЗаполнено(Объект.Ответственный) И ТипЗнч(Объект.Ответственный) = Тип("СправочникСсылка.Пользователи") Тогда
	Попытка
		ВидКИ = Справочники.ВидыКонтактнойИнформации["EmailПользователя"];
		НайденаяСтрока = Объект.Ответственный.КонтактнаяИнформация.Найти(ВидКИ,"Вид");
	Исключение
		НайденаяСтрока = Объект.Ответственный.КонтактнаяИнформация.Найти(Перечисления.ТипыКонтактнойИнформации["АдресЭлектроннойПочты"],"Тип");
	КонецПопытки;
	Если НЕ НайденаяСтрока = Неопределено Тогда
		Результат = НайденаяСтрока.Представление;
	КонецЕсли;
КонецЕсли;</parameter>
				</c>
			</c>
		</row>
