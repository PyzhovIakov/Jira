				<c>
					<f>12</f>
					<parameter>Контактное лицо (должность) (родительный падеж)</parameter>
				</c>
			</c>
			<c>
				<c>
					<f>13</f>
					<parameter>ДолжностьКонтактноеЛицоРодПад</parameter>
				</c>
			</c>
			<c>
				<c>
					<f>14</f>
					<parameter>Если ЗначениеЗаполнено(Объект.КонтактноеЛицо) Тогда
		Результат = Объект.КонтактноеЛицо.CRM_Должность;
	ИначеЕсли ЗначениеЗаполнено(Объект.Партнер) Тогда
		Результат = "";
		Если ЗначениеЗаполнено(Объект.Партнер.CRM_ОсновноеКонтактноеЛицо) Тогда
			Результат = Объект.Партнер.CRM_ОсновноеКонтактноеЛицо.CRM_Должность;
		Иначе
			Запрос = Новый Запрос;
			Запрос.Текст = "ВЫБРАТЬ ПЕРВЫЕ 1
	               |	КонтактныеЛица.Ссылка КАК КонтактноеЛицо
	               |ИЗ
	               |	Справочник.КонтактныеЛицаПартнеров КАК КонтактныеЛица
	               |ГДЕ
	               |	(НЕ КонтактныеЛица.ПометкаУдаления)
	               |	И КонтактныеЛица.Владелец = &amp;Владелец";
			Запрос.УстановитьПараметр("Владелец", Объект.Партнер);
	
			Выборка = Запрос.Выполнить().Выбрать();
			Если Выборка.Следующий() Тогда
				Результат = Выборка.КонтактноеЛицо.CRM_Должность;
			КонецЕсли;
		КонецЕсли;
Иначе
Результат = "";
	КонецЕсли;
Результат = Строка(Результат);
Результат = ПолучитьСклоненияСтроки(Результат,,"ПД=Родительный");
Если ТипЗнч(Результат) = Тип("Массив") И Результат.Количество()&gt;0 Тогда
	Результат = Результат[0];
Иначе
	Результат = "";
КонецЕсли;</parameter>
				</c>
			</c>
