    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>ДСПоКампаниям.CRM_РекламнаяКампания</segments>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВЫБОР КОГДА ЕСТЬNULL(ИсточникиИнтереса.ИсточникПервичногоИнтереса, НЕОПРЕДЕЛЕНО) = НЕОПРЕДЕЛЕНО ТОГДА
		  ЗНАЧЕНИЕ(Справочник.МаркетинговыеМероприятия.ПустаяСсылка)
	ИНАЧЕ ИсточникиИнтереса.ИсточникПервичногоИнтереса
	КОНЕЦ КАК ИсточникПервичногоИнтереса,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ВЫРАЗИТЬ(Документы.Объект КАК Документ.CRM_Заявка)) КАК ДокументЗаявка,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ВЫРАЗИТЬ(Документы.Объект КАК Документ.ТелефонныйЗвонок)) КАК ДокументТелефонныйЗвонок,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ВЫРАЗИТЬ(Документы.Объект КАК Документ.CRM_Интерес)) КАК ДокументИнтерес
ПОМЕСТИТЬ ИсточникиПервичногоИнтереса
ИЗ
	РегистрСведений.CRM_ЖурналДокументов КАК Документы
	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИсточникиПервичногоИнтереса КАК ИсточникиИнтереса
	ПО (ИсточникиИнтереса.Сделка = Документы.Объект)
ГДЕ
	Документы.Дата МЕЖДУ &amp;Период1 И &amp;Период2

СГРУППИРОВАТЬ ПО
	ВЫБОР КОГДА ЕСТЬNULL(ИсточникиИнтереса.ИсточникПервичногоИнтереса, НЕОПРЕДЕЛЕНО) = НЕОПРЕДЕЛЕНО ТОГДА
		  ЗНАЧЕНИЕ(Справочник.МаркетинговыеМероприятия.ПустаяСсылка)
	ИНАЧЕ ИсточникиИнтереса.ИсточникПервичногоИнтереса
	КОНЕЦ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	CRM_РекламнаяКампания КАК CRM_РекламнаяКампания,
	Сайт,
	СУММА(СредняяЦенаКлика * Клики) КАК Расходы
ПОМЕСТИТЬ ЗатратыРекламныхКампаний
ИЗ
	РегистрСведений.CRM_ЗатратыРекламныхКампаний
ГДЕ
	Дата МЕЖДУ &amp;Период1 И &amp;Период2	
	И ИсточникиКампаний.Включено
	И ВЫБОР КОГДА &amp;ФильтрРекламныеКампании ТОГДА CRM_РекламнаяКампания В (&amp;РекламныеКампании) ИНАЧЕ ИСТИНА КОНЕЦ

СГРУППИРОВАТЬ ПО
	CRM_РекламнаяКампания,
	Сайт
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПоказателиКампаний.CRM_Сайт КАК CRM_Сайт,
	ВЫБОР КОГДА ПоказателиКампаний.CRM_РекламнаяКампания = ЗНАЧЕНИЕ(Справочник.МаркетинговыеМероприятия.ПустаяСсылка) 
			ИЛИ ПоказателиКампаний.CRM_РекламнаяКампания.CRM_РодительскаяКампания = ЗНАЧЕНИЕ(Справочник.МаркетинговыеМероприятия.ПустаяСсылка) ТОГДА
		  ПоказателиКампаний.CRM_РекламнаяКампания
	ИНАЧЕ ПоказателиКампаний.CRM_РекламнаяКампания.CRM_РодительскаяКампания
	КОНЕЦ КАК CRM_РекламнаяКампания,
	СУММА(ПоказателиКампаний.Визиты) КАК Визиты,
	СУММА(ПоказателиКампаний.Посетители) КАК Посетители,
