  <attributes>
    <name>ИнтересыКлиента</name>
    <title>
      <key>ru</key>
      <value>Интересы клиента</value>
    </title>
    <title>
      <key>en</key>
      <value>Customer Leads</value>
    </title>
    <id>44</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>ИнтересыКлиента.Ответственный</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>ИнтересыКлиента.СостояниеИнтереса</segments>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	CRM_Интерес.Ссылка КАК Интерес,
	CRM_Интерес.СостояниеИнтереса.Родитель КАК Сценарий,
	CRM_Интерес.ТипУслуги КАК ТипУслуги,
	ВЫБОР
		КОГДА CRM_Интерес.Партнер = ЗНАЧЕНИЕ(Справочник.Партнеры.ПустаяСсылка)
				И НЕ CRM_Интерес.ПотенциальныйКлиент = ЗНАЧЕНИЕ(Справочник.CRM_потенциальныеКлиенты.ПустаяСсылка)
			ТОГДА &amp;НадписьПотенциальногоКлиента
		ИНАЧЕ CRM_Интерес.Партнер
	КОНЕЦ КАК Клиент,
	ВЫБОР
		КОГДА CRM_Интерес.Партнер = ЗНАЧЕНИЕ(Справочник.Партнеры.ПустаяСсылка)
				И НЕ CRM_Интерес.ПотенциальныйКлиент = ЗНАЧЕНИЕ(Справочник.CRM_потенциальныеКлиенты.ПустаяСсылка)
				И CRM_ИнтересКонтакты.Контакт ЕСТЬ NULL
			ТОГДА CRM_Интерес.ПотенциальныйКлиент
		ИНАЧЕ ЕСТЬNULL(CRM_ИнтересКонтакты.Контакт, CRM_ИнтересКонтакты2.Контакт)
	КОНЕЦ КАК Контакт,
	CRM_ИнтересКонтакты.Роль КАК Роль,
	ВЫБОР
		КОГДА CRM_Интерес.Партнер = ЗНАЧЕНИЕ(Справочник.Партнеры.ПустаяСсылка)
				И НЕ CRM_Интерес.ПотенциальныйКлиент = ЗНАЧЕНИЕ(Справочник.CRM_потенциальныеКлиенты.ПустаяСсылка)
				И CRM_ИнтересКонтакты.Контакт ЕСТЬ NULL
			ТОГДА CRM_Интерес.ПотенциальныйКлиент
		ИНАЧЕ ЕСТЬNULL(CRM_ИнтересКонтакты.Контакт, CRM_ИнтересКонтакты2.Контакт)
	КОНЕЦ = &amp;Контакт КАК ИнтересПоТекКонтакту,
	ВЫБОР
		КОГДА CRM_Интерес.СостояниеИнтереса.ВидСостояния = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСостоянияИнтереса.УспешноеЗавершение)
			ТОГДА 1
		ИНАЧЕ ВЫБОР
				КОГДА CRM_Интерес.СостояниеИнтереса.ВидСостояния = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСостоянияИнтереса.НеудачноеЗавершение)
					ТОГДА 2
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ КАК КартинкаСостояния,
	CRM_Интерес.Ответственный КАК Ответственный,
	CRM_Интерес.СостояниеИнтереса КАК СостояниеИнтереса
ИЗ
	Документ.CRM_Интерес КАК CRM_Интерес
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.CRM_Интерес.Контакты КАК CRM_ИнтересКонтакты
		ПО (CRM_ИнтересКонтакты.Ссылка = CRM_Интерес.Ссылка)
			И (CRM_ИнтересКонтакты.Контакт = &amp;Контакт)
			И (&amp;Контакт &lt;> ЗНАЧЕНИЕ(Справочник.КонтактныеЛицаПартнеров.ПустаяСсылка))
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.CRM_Интерес.Контакты КАК CRM_ИнтересКонтакты2
		ПО (CRM_ИнтересКонтакты2.Ссылка = CRM_Интерес.Ссылка)
			И (CRM_ИнтересКонтакты2.Контакт = CRM_Интерес.КонтактноеЛицо)
ГДЕ
	НЕ CRM_Интерес.ПометкаУдаления
	И (CRM_Интерес.Партнер = &amp;Клиент
			ИЛИ CRM_Интерес.ПотенциальныйКлиент = &amp;Клиент)
	И (&amp;НетОтбораПоАктивнымИнтересам
			ИЛИ НЕ CRM_Интерес.Завершен)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	CRM_ИнтересКонтакты.Ссылка,
	NULL,
	NULL,
	ВЫБОР
		КОГДА CRM_ИнтересКонтакты.Ссылка.Партнер = ЗНАЧЕНИЕ(Справочник.Партнеры.ПустаяСсылка)
				И НЕ CRM_ИнтересКонтакты.Ссылка.ПотенциальныйКлиент = ЗНАЧЕНИЕ(Справочник.CRM_потенциальныеКлиенты.ПустаяСсылка)
			ТОГДА CRM_ИнтересКонтакты.Ссылка.ПотенциальныйКлиент
		ИНАЧЕ CRM_ИнтересКонтакты.Ссылка.Партнер
	КОНЕЦ,
	CRM_ИнтересКонтакты.Контакт,
	CRM_ИнтересКонтакты.Роль,
	ИСТИНА,
	ВЫБОР
		КОГДА CRM_ИнтересКонтакты.Ссылка.СостояниеИнтереса.ВидСостояния = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСостоянияИнтереса.УспешноеЗавершение)
			ТОГДА 1
		ИНАЧЕ ВЫБОР
				КОГДА CRM_ИнтересКонтакты.Ссылка.СостояниеИнтереса.ВидСостояния = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСостоянияИнтереса.НеудачноеЗавершение)
					ТОГДА 2
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ,
	CRM_ИнтересКонтакты.Ссылка.Ответственный,
	CRM_ИнтересКонтакты.Ссылка.СостояниеИнтереса
ИЗ
	Документ.CRM_Интерес.Контакты КАК CRM_ИнтересКонтакты
ГДЕ
	НЕ CRM_ИнтересКонтакты.Ссылка.ПометкаУдаления
	И CRM_ИнтересКонтакты.Ссылка.Партнер &lt;> &amp;Клиент
	И CRM_ИнтересКонтакты.Ссылка.ПотенциальныйКлиент &lt;> &amp;Клиент
	И (&amp;НетОтбораПоАктивнымИнтересам
			ИЛИ НЕ CRM_ИнтересКонтакты.Ссылка.Завершен)
	И CRM_ИнтересКонтакты.Контакт = &amp;Контакт</queryText>
