    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВЫБОР
		КОГДА ЖурналОбъектов.Проведен
				И ЖурналОбъектов.ПометкаУдаления
			ТОГДА 3
		КОГДА НЕ ЖурналОбъектов.Проведен
				И ЖурналОбъектов.ПометкаУдаления
			ТОГДА 2
		КОГДА ЖурналОбъектов.Проведен
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК СостояниеОбъектаИндексКартинки,
	ВЫБОР
		КОГДА ЖурналОбъектов.ЕстьПрисоединенныеФайлы
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ЕстьПрисоединенныеФайлыИндексКартинки,
	ВЫБОР
		КОГДА ЖурналОбъектов.Важность = ЗНАЧЕНИЕ(Перечисление.ВариантыВажностиВзаимодействия.Низкая)
			ТОГДА 0
		КОГДА ЖурналОбъектов.Важность = ЗНАЧЕНИЕ(Перечисление.ВариантыВажностиВзаимодействия.Высокая)
			ТОГДА 2
		ИНАЧЕ 1
	КОНЕЦ КАК ВажностьИндексКартинки,
	ВЫБОР
		КОГДА ЖурналОбъектов.ВидСобытия = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСобытий.ТелефонныйЗвонок)
				И ЖурналОбъектов.ВходящееИсходящее = ЗНАЧЕНИЕ(Перечисление.CRM_ВходящееИсходящееСобытие.Входящее)
			ТОГДА 1
		КОГДА ЖурналОбъектов.ВидСобытия = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСобытий.ТелефонныйЗвонок)
				И ЖурналОбъектов.ВходящееИсходящее = ЗНАЧЕНИЕ(Перечисление.CRM_ВходящееИсходящееСобытие.Исходящее)
			ТОГДА 2
		КОГДА ЖурналОбъектов.ВидСобытия = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСобытий.ЛичнаяВстреча)
				И ЖурналОбъектов.ВходящееИсходящее = ЗНАЧЕНИЕ(Перечисление.CRM_ВходящееИсходящееСобытие.Входящее)
			ТОГДА 3
		КОГДА ЖурналОбъектов.ВидСобытия = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСобытий.ЛичнаяВстреча)
				И ЖурналОбъектов.ВходящееИсходящее = ЗНАЧЕНИЕ(Перечисление.CRM_ВходящееИсходящееСобытие.Исходящее)
			ТОГДА 4
		КОГДА ЖурналОбъектов.ВидСобытия = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСобытий.ЭлектронноеПисьмо)
					И ЖурналОбъектов.ВходящееИсходящее = ЗНАЧЕНИЕ(Перечисление.CRM_ВходящееИсходящееСобытие.Входящее)
				ИЛИ ЖурналОбъектов.Объект ССЫЛКА Документ.ЭлектронноеПисьмоВходящее
			ТОГДА 5
		КОГДА ЖурналОбъектов.ВидСобытия = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСобытий.ЭлектронноеПисьмо)
					И ЖурналОбъектов.ВходящееИсходящее = ЗНАЧЕНИЕ(Перечисление.CRM_ВходящееИсходящееСобытие.Исходящее)
				ИЛИ ЖурналОбъектов.Объект ССЫЛКА Документ.ЭлектронноеПисьмоИсходящее
			ТОГДА 6
		КОГДА ЖурналОбъектов.ВидСобытия = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСобытий.ПочтовоеПисьмо)
				И ЖурналОбъектов.ВходящееИсходящее = ЗНАЧЕНИЕ(Перечисление.CRM_ВходящееИсходящееСобытие.Входящее)
			ТОГДА 7
		КОГДА ЖурналОбъектов.ВидСобытия = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСобытий.ПочтовоеПисьмо)
				И ЖурналОбъектов.ВходящееИсходящее = ЗНАЧЕНИЕ(Перечисление.CRM_ВходящееИсходящееСобытие.Исходящее)
			ТОГДА 8
		КОГДА ЖурналОбъектов.ВидСобытия = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСобытий.Прочее)
				И ЖурналОбъектов.ВходящееИсходящее = ЗНАЧЕНИЕ(Перечисление.CRM_ВходящееИсходящееСобытие.Входящее)
			ТОГДА 9
		КОГДА ЖурналОбъектов.ВидСобытия = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСобытий.Прочее)
				И ЖурналОбъектов.ВходящееИсходящее = ЗНАЧЕНИЕ(Перечисление.CRM_ВходящееИсходящееСобытие.Исходящее)
			ТОГДА 10
		ИНАЧЕ 0
	КОНЕЦ КАК ВидОбъектаИндексКартинки,
	ВЫБОР
		КОГДА ЖурналОбъектов.СостояниеСобытия.ТипСостояния = ЗНАЧЕНИЕ(Перечисление.CRM_ТипыСостоянийСобытий.ВРаботе)
			ТОГДА 4
		КОГДА ЖурналОбъектов.СостояниеСобытия.ТипСостояния = ЗНАЧЕНИЕ(Перечисление.CRM_ТипыСостоянийСобытий.Запланировано)
			ТОГДА 3
		КОГДА ЖурналОбъектов.СостояниеСобытия.ТипСостояния = ЗНАЧЕНИЕ(Перечисление.CRM_ТипыСостоянийСобытий.Завершено)
			ТОГДА 1
		КОГДА ЖурналОбъектов.СостояниеСобытия.ТипСостояния = ЗНАЧЕНИЕ(Перечисление.CRM_ТипыСостоянийСобытий.Отменено)
