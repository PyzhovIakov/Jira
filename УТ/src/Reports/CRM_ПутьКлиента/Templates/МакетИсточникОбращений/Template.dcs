		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	CRM_СостоянияИнтересов.Ссылка КАК Ссылка,
	CRM_СостоянияИнтересов.РеквизитДопУпорядочивания КАК Порядок
ПОМЕСТИТЬ СостоянияИнтересов
ИЗ
	Справочник.CRM_СостоянияИнтересов КАК CRM_СостоянияИнтересов
ГДЕ
	CRM_СостоянияИнтересов.Родитель = &amp;Сценарий
	И CRM_СостоянияИнтересов.ВидСостояния &lt;&gt; &amp;ВидСостояния
	И CRM_СостоянияИнтересов.Используется
	И НЕ CRM_СостоянияИнтересов.ПометкаУдаления

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	"Обращения",
	-2

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	"Лиды",
	-1
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	CRM_СостоянияЛидов.Лид КАК Документ,
	ВЫБОР
		КОГДА CRM_СостоянияЛидов.ВидКанала = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыКаналовПоступленияОбращений.ТелефонныйЗвонок)
			ТОГДА "Телефония"
		ИНАЧЕ CRM_СостоянияЛидов.КаналПолучения
	КОНЕЦ КАК Источник,
	ВЫБОР
		КОГДА НЕ CRM_СостоянияЛидов.Состояние В (&amp;СостоянияПовторного)
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК ЭтоЛид
ПОМЕСТИТЬ ОбращенияИЛиды
ИЗ
	РегистрСведений.CRM_СостоянияЛидов КАК CRM_СостоянияЛидов
ГДЕ
	ВЫБОР
			КОГДА CRM_СостоянияЛидов.Лид ССЫЛКА Справочник.CRM_Диалоги
				ТОГДА CRM_СостоянияЛидов.Лид.ДатаНачала
			ИНАЧЕ CRM_СостоянияЛидов.Лид.Дата
		КОНЕЦ МЕЖДУ &amp;НачалоПериода И &amp;ОкончаниеПериода
	И CRM_СостоянияЛидов.Состояние &lt;&gt; &amp;СостояниеОтклонен
	И (CRM_СостоянияЛидов.ОтветственныйЗаОбработку В (&amp;Менеджеры)
			ИЛИ CRM_СостоянияЛидов.ОтветственныйЗаОбработку В (&amp;Роли))
	И (CRM_СостоянияЛидов.ВидКанала = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыКаналовПоступленияОбращений.ТелефонныйЗвонок)
			ИЛИ CRM_СостоянияЛидов.КаналПолучения В (&amp;ИсточникиЛидов))
	И НЕ CRM_СостоянияЛидов.Лид ССЫЛКА Документ.CRM_СообщениеМессенджера
	И НЕ CRM_СостоянияЛидов.Лид.ПометкаУдаления
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	CRM_Интерес.Ссылка КАК Ссылка,
	КОЛИЧЕСТВО(КалендарныеГрафики.ДатаГрафика) КАК СрокНаЭтапе
ПОМЕСТИТЬ ДлительностиАктивныхЭтапов
ИЗ
	Документ.CRM_Интерес КАК CRM_Интерес
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.CRM_ИсторияСостоянийОбращений.СрезПоследних КАК CRM_ИсторияСостоянийОбращенийСрезПоследних
			ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.КалендарныеГрафики КАК КалендарныеГрафики
			ПО CRM_ИсторияСостоянийОбращенийСрезПоследних.Период &lt;= КалендарныеГрафики.ДатаГрафика
				И (КалендарныеГрафики.ДеньВключенВГрафик)
				И (КалендарныеГрафики.ДатаГрафика &lt;= &amp;ТекущаяДата)
				И (КалендарныеГрафики.Календарь = &amp;Календарь)
		ПО CRM_Интерес.Ссылка = CRM_ИсторияСостоянийОбращенийСрезПоследних.Обращение
ГДЕ
	НЕ CRM_Интерес.Завершен
	И НЕ CRM_Интерес.ПометкаУдаления
	И CRM_Интерес.Дата &gt;= &amp;НачалоПериода
	И CRM_Интерес.Дата &lt;= &amp;ОкончаниеПериода

СГРУППИРОВАТЬ ПО
	CRM_Интерес.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СостоянияИнтересов.Ссылка КАК Состояние,
	ОбращенияИЛиды.Документ КАК Документ,
	ОбращенияИЛиды.Документ КАК ОбъектСсылка,
	ОбращенияИЛиды.Источник КАК Источник,
	0 КАК Сумма,
	0 КАК СконвертированныхИнтересов,
	0 КАК СуммаСконвертированныхИнтересов,
	СостоянияИнтересов.Порядок КАК Порядок
ПОМЕСТИТЬ втИсточникиОбращений
ИЗ
	СостоянияИнтересов КАК СостоянияИнтересов
		ЛЕВОЕ СОЕДИНЕНИЕ ОбращенияИЛиды КАК ОбращенияИЛиды
		ПО (СостоянияИнтересов.Ссылка = "Обращения")

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	СостоянияИнтересов.Ссылка,
	ОбращенияИЛиды.Документ,
	ОбращенияИЛиды.Документ,
	ОбращенияИЛиды.Источник,
	0,
	0,
	0,
	СостоянияИнтересов.Порядок
ИЗ
	СостоянияИнтересов КАК СостоянияИнтересов
		ЛЕВОЕ СОЕДИНЕНИЕ ОбращенияИЛиды КАК ОбращенияИЛиды
		ПО (СостоянияИнтересов.Ссылка = "Лиды")
ГДЕ
	ОбращенияИЛиды.ЭтоЛид

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	СостоянияИнтересов.Ссылка,
	CRM_Интерес.Ссылка,
	CRM_Интерес.Ссылка,
	ВЫБОР
		КОГДА CRM_Интерес.ИсточникОбращения = НЕОПРЕДЕЛЕНО
			ТОГДА "Без источника"
		ИНАЧЕ CRM_Интерес.ИсточникОбращения
	КОНЕЦ,
	ЕСТЬNULL(CRM_Интерес.ОжидаемаяВыручка, 0),
	ВЫБОР
		КОГДА CRM_СостоянияЛидов.Состояние ЕСТЬ NULL
			ТОГДА 0
		ИНАЧЕ 1
	КОНЕЦ,
	ВЫБОР
		КОГДА CRM_СостоянияЛидов.Состояние ЕСТЬ NULL
			ТОГДА 0
		ИНАЧЕ CRM_Интерес.ОжидаемаяВыручка
	КОНЕЦ,
	СостоянияИнтересов.Порядок
ИЗ
	СостоянияИнтересов КАК СостоянияИнтересов
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.CRM_Интерес КАК CRM_Интерес
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.CRM_СостоянияЛидов КАК CRM_СостоянияЛидов
			ПО (CRM_СостоянияЛидов.СконвертированВОбъект = CRM_Интерес.Ссылка)
				И (CRM_СостоянияЛидов.Состояние = &amp;СостояниеСконвертированного)
		ПО СостоянияИнтересов.Ссылка = CRM_Интерес.СостояниеИнтереса
			И (CRM_Интерес.Дата МЕЖДУ &amp;НачалоПериода И &amp;ОкончаниеПериода)
			И (НЕ CRM_Интерес.ПометкаУдаления)
			И (CRM_Интерес.Ответственный В (&amp;Менеджеры)
				ИЛИ CRM_Интерес.Ответственный В (&amp;Роли))
			И (СостоянияИнтересов.Ссылка &lt;&gt; "Обращения")
			И (CRM_Интерес.ОжидаемаяВыручка МЕЖДУ &amp;СуммаОт И &amp;СуммаДо)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	втИсточникиОбращений.Состояние КАК Состояние,
	втИсточникиОбращений.Документ КАК Документ,
	втИсточникиОбращений.ОбъектСсылка КАК ОбъектСсылка,
	втИсточникиОбращений.Источник КАК Источник,
	втИсточникиОбращений.Сумма КАК Сумма,
	втИсточникиОбращений.СконвертированныхИнтересов КАК СконвертированныхИнтересов,
	втИсточникиОбращений.СуммаСконвертированныхИнтересов КАК СуммаСконвертированныхИнтересов,
	втИсточникиОбращений.Порядок КАК Порядок,
	ДлительностиАктивныхЭтапов.СрокНаЭтапе КАК СрокНаЭтапе
ИЗ
	втИсточникиОбращений КАК втИсточникиОбращений
		ЛЕВОЕ СОЕДИНЕНИЕ ДлительностиАктивныхЭтапов КАК ДлительностиАктивныхЭтапов
		ПО втИсточникиОбращений.ОбъектСсылка = ДлительностиАктивныхЭтапов.Ссылка</query>
	</dataSet>
	<totalField>
		<dataPath>Документ</dataPath>
		<expression>Количество(Различные Документ)</expression>
		<group>Источник</group>
		<group>Состояние</group>
	</totalField>
	<totalField>
		<dataPath>Порядок</dataPath>
		<expression>Максимум(Порядок)</expression>
		<group>Состояние</group>
	</totalField>
	<totalField>
		<dataPath>Сумма</dataPath>
		<expression>Сумма(Сумма)</expression>
	</totalField>
	<totalField>
		<dataPath>СконвертированныхИнтересов</dataPath>
		<expression>Сумма(СконвертированныхИнтересов)</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаСконвертированныхИнтересов</dataPath>
		<expression>Сумма(СуммаСконвертированныхИнтересов)</expression>
	</totalField>
	<totalField>
		<dataPath>СрокНаЭтапе</dataPath>
		<expression>Среднее(СрокНаЭтапе)</expression>
		<group>Источник</group>
		<group>Состояние</group>
	</totalField>
	<parameter>
		<name>НачалоПериода</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Начало периода</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
				<v8:DateFractions>DateTime</v8:DateFractions>
			</v8:DateQualifiers>
		</valueType>
		<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
		<useRestriction>true</useRestriction>
		<expression>&amp;ПериодОтчета.ДатаНачала</expression>
	</parameter>
