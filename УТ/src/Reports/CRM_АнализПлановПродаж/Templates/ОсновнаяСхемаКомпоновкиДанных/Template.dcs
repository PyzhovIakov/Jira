	ВЫБОР
		КОГДА &amp;ПодразделениеПродаж = ЗНАЧЕНИЕ(Справочник.СтруктураПредприятия.ПустаяСсылка)
			ТОГДА &amp;ПодразделениеПродаж
		ИНАЧЕ CRM_ПродажиОбороты.Подразделение
	КОНЕЦ КАК Подразделение,
	ВЫБОР
		КОГДА &amp;МенеджерПродаж = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
			ТОГДА &amp;МенеджерПродаж
		ИНАЧЕ CRM_ПродажиОбороты.Менеджер
	КОНЕЦ КАК Менеджер,
	ВЫБОР
		КОГДА &amp;ПартнерПродаж = ЗНАЧЕНИЕ(Справочник.Партнеры.ПустаяСсылка)
			ТОГДА &amp;ПартнерПродаж
		ИНАЧЕ CRM_ПродажиОбороты.АналитикаУчетаПоПартнерам.Партнер
	КОНЕЦ КАК Партнер,
	ВЫБОР
		КОГДА &amp;НоменклатураПродаж = ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)
			ТОГДА &amp;НоменклатураПродаж
		ИНАЧЕ CRM_ПродажиОбороты.АналитикаУчетаНоменклатуры.Номенклатура
	КОНЕЦ КАК Номенклатура,
	ВЫБОР
		КОГДА &amp;СкладПродаж = ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка)
			ТОГДА &amp;СкладПродаж
		ИНАЧЕ CRM_ПродажиОбороты.Склад
	КОНЕЦ КАК Склад,
	ВЫБОР
		КОГДА &amp;БизнесРегионПродаж = НЕОПРЕДЕЛЕНО
			ТОГДА &amp;БизнесРегионПродаж
		ИНАЧЕ CRM_ПродажиОбороты.АналитикаУчетаПоПартнерам.Партнер.БизнесРегион
	КОНЕЦ КАК БизнесРегион,
	ВЫБОР
		КОГДА &amp;ОсновнаяОтрасльПродаж = НЕОПРЕДЕЛЕНО
			ТОГДА &amp;ОсновнаяОтрасльПродаж
		ИНАЧЕ CRM_ПродажиОбороты.АналитикаУчетаПоПартнерам.Партнер.CRM_ОсновнаяОтрасль
	КОНЕЦ КАК ОсновнаяОтрасль,
	ВЫБОР
		КОГДА &amp;СегментРынкаПродаж = НЕОПРЕДЕЛЕНО
			ТОГДА &amp;СегментРынкаПродаж
		ИНАЧЕ CRM_ПродажиОбороты.АналитикаУчетаПоПартнерам.Партнер.CRM_СегментРынка
	КОНЕЦ КАК СегментРынка,
	ВЫБОР
		КОГДА &amp;ТоварнаяКатегорияПродаж = НЕОПРЕДЕЛЕНО
			ТОГДА &amp;ТоварнаяКатегорияПродаж
		ИНАЧЕ CRM_ПродажиОбороты.АналитикаУчетаНоменклатуры.Номенклатура.ТоварнаяКатегория
	КОНЕЦ КАК ТоварнаяКатегория,
	ВЫБОР
		КОГДА &amp;ВидНоменклатурыПродаж = НЕОПРЕДЕЛЕНО
			ТОГДА &amp;ВидНоменклатурыПродаж
		ИНАЧЕ CRM_ПродажиОбороты.АналитикаУчетаНоменклатуры.Номенклатура.ВидНоменклатуры
	КОНЕЦ КАК ВидНоменклатуры,
	ВЫБОР
		КОГДА &amp;НаправлениеДеятельностиПродаж = НЕОПРЕДЕЛЕНО
			ТОГДА &amp;НаправлениеДеятельностиПродаж
		ИНАЧЕ CRM_ПродажиОбороты.НаправлениеДеятельностиНоменклатуры
	КОНЕЦ КАК НаправлениеДеятельности,
	CRM_ПродажиОбороты.КоличествоОборот КАК КоличествоФакт,
	CRM_ПродажиОбороты.СуммаВыручкиОборот КАК СуммаФакт
ИЗ
	РегистрНакопления.ВыручкаИСебестоимостьПродаж.Обороты(
			&amp;НачалоПериода,
			&amp;КонецПериода,
			Период,
			(&amp;МенеджерПродаж = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
				ИЛИ Менеджер = &amp;МенеджерПродаж)
				И (&amp;ПодразделениеПродаж = ЗНАЧЕНИЕ(Справочник.СтруктураПредприятия.ПустаяСсылка)
					ИЛИ Подразделение = &amp;ПодразделениеПродаж)
				И (&amp;СкладПродаж = ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка)
					ИЛИ Склад = &amp;СкладПродаж)
				И (&amp;ПартнерПродаж = ЗНАЧЕНИЕ(Справочник.Партнеры.ПустаяСсылка)
					ИЛИ АналитикаУчетаПоПартнерам.Партнер = &amp;ПартнерПродаж)
				И (&amp;НоменклатураПродаж = ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)
					ИЛИ АналитикаУчетаНоменклатуры.Номенклатура = &amp;НоменклатураПродаж)
				И (&amp;ТоварнаяКатегорияПродаж = НЕОПРЕДЕЛЕНО
					ИЛИ АналитикаУчетаНоменклатуры.Номенклатура.ТоварнаяКатегория = &amp;ТоварнаяКатегорияПродаж)
				И (&amp;НаправлениеДеятельностиПродаж = НЕОПРЕДЕЛЕНО
					ИЛИ НаправлениеДеятельностиНоменклатуры = &amp;НаправлениеДеятельностиПродаж)
				И (&amp;ВидНоменклатурыПродаж = НЕОПРЕДЕЛЕНО
					ИЛИ АналитикаУчетаНоменклатуры.Номенклатура.ВидНоменклатуры = &amp;ВидНоменклатурыПродаж)
				И (&amp;БизнесРегионПродаж = НЕОПРЕДЕЛЕНО
					ИЛИ АналитикаУчетаПоПартнерам.Партнер.БизнесРегион = &amp;БизнесРегионПродаж)
				И (&amp;ОсновнаяОтрасльПродаж = НЕОПРЕДЕЛЕНО
					ИЛИ АналитикаУчетаПоПартнерам.Партнер.CRM_ОсновнаяОтрасль = &amp;ОсновнаяОтрасльПродаж)
				И (&amp;СегментРынкаПродаж = НЕОПРЕДЕЛЕНО
					ИЛИ АналитикаУчетаПоПартнерам.Партнер.CRM_СегментРынка = &amp;СегментРынкаПродаж)) КАК CRM_ПродажиОбороты
					</query>
	</dataSet>
